{
	"registry.docker.io" : {
		"groups" : {
			"or_admin" : ["iki"],
			"or_students" : ["pero", "jozo", "ante", "mate"],
			"or_lab1" : ["pero", "jozo", "ante", "mate"],
			"or_lab2a" : ["pero", "jozo"],
			"or_lab2b" : ["ante", "mate"]
		},
		"repository" : {
			"or/lab/images/[A-Za-z0-9]+" : {
				"(or_admin)" : ["push", "pull"]
			},
			"or/lab/images/lab1" : {
				"(or_students)" : ["pull"]
			},
			"or/lab/images/lab2a" : {
				"(or_lab2a)" :["pull"]	
			},
			"or/lab/images/lab2b" : {
				"(or_lab2a)" :["pull"]	
			},
			"or/lab/*" : {
				"(or_admin)" : ["pull"]
			},
			"or/lab/<USERNAME>/lab1" : {
				"(or_students)" : ["push", "pull"]  
			},
			"or/lab/<USERNAME>/lab2" : {
				"(or_students)" :  ["push", "pull"]
			}
		}
	}
}
